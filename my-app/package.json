{
  "name": "ai-guided-saas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:lighthouse": "lhci autorun",
    "test:integration": "jest --testPathPattern=integration",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "deploy:check": "node scripts/error-prevention-pipeline.js",
    "deploy:staging": "vercel --token=$VERCEL_TOKEN",
    "deploy:production": "vercel --prod --token=$VERCEL_TOKEN",
    "analyze": "cross-env ANALYZE=true next build",
    "error-prevention": "node scripts/error-prevention-pipeline.js",
    "claude:init": "echo \"🧠 Initializing Claude Code integration...\"",
    "claude:sync": "echo \"🔄 Syncing documentation...\"",
    "claude:compact": "echo \"🗜️ Optimizing context window...\"",
    "claude:status": "echo \"📊 Checking Claude Code status...\""
  },
  "dependencies": {
    "@next-auth/supabase-adapter": "^0.2.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@xyflow/react": "^12.8.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "next": "^14.2.30",
    "next-auth": "^4.24.11",
    "react": "^18",
    "react-dom": "^18",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.23",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
