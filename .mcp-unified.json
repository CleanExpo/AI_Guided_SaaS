{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "--node-options=--experimental-vm-modules", "@upstash/context7-mcp@1.0.6"],
      "env": {}
    },
    "sequentialthinking": {
      "type": "stdio", 
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "env": {}
    },
    "memory": {
      "type": "stdio",
      "command": "npx", 
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "env": {}
    },
    "stripe": {
      "type": "stdio",
      "command": "node",
      "args": ["/mnt/c/Users/Disaster Recovery 4/Documents/Cline/MCP/stripe-agent-toolkit-server/dist/index.js"],
      "env": {}
    },
    "fetch-mcp": {
      "type": "stdio",
      "command": "node", 
      "args": ["/mnt/c/Users/Disaster Recovery 4/Documents/Cline/MCP/fetch-mcp/dist/index.js"],
      "env": {}
    },
    "github": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {}
    },
    "magic-21st": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@21st-dev/magic@latest"],
      "env": {
        "API_KEY": "$MAGIC_21ST_API_KEY"
      }
    },
    "bright-data": {
      "type": "stdio",
      "command": "npx",
      "args": ["@brightdata/mcp"],
      "env": {
        "API_TOKEN": "$BRIGHTDATA_API_KEY",
        "RATE_LIMIT": "100/1h"
      }
    },
    "puppeteer": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"],
      "env": {
        "PUPPETEER_LAUNCH_OPTIONS": "{\"headless\": false, \"args\": []}",
        "ALLOW_DANGEROUS": "true"
      }
    },
    "digitalocean": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@digitalocean/mcp-server"],
      "env": {
        "DIGITALOCEAN_TOKEN": "$DIGITALOCEAN_TOKEN"
      }
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["@playwright/mcp@latest", "--headless"],
      "env": {
        "PLAYWRIGHT_BROWSERS_PATH": "/home/$USER/.cache/playwright",
        "DISPLAY": ":99"
      }
    },
    "jina-scraper": {
      "type": "stdio",
      "command": "npx",
      "args": ["@jinaai/mcp-scraper@latest"],
      "env": {
        "JINA_API_KEY": "$JINA_API_KEY"
      }
    }
  }
}
